{
  "name": "shirajuki",
  "version": "1.0.0",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "deploy": "npm run build && gh-pages -d build -b gh-pages -t true",
    "package": "svelte-kit package",
